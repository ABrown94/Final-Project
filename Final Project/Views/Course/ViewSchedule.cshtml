@model IEnumerable<Final_Project.Models.Course_Schedule>

<h4>Schedule
<span style="font-size:small">@Html.ActionLink("Edit", "EditCourseSchedule", new { id =  Model.First().CourseId  })</span>
    </h4> 

<table class="table-condensed table-bordered">
    <tr>
        
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DayOfWeek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime) - @Html.DisplayFor(modelItem => item.EndTime) 
        </td>
    </tr>
}
    @*<input type="hidden" value="@ViewBag.hdnFlag" id="hdnFlag" />*@
</table>
